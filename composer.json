{
	"name": "webarq/presence",
	"description": "A Laravel application skeleton based on https://github.com/laravel/laravel/tree/v4.0.9.",
	"keywords": ["framework", "laravel"],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@git2.webarq.com:/webarq/site.git"
		},
		{
			"type": "vcs",
			"url": "git@git2.webarq.com:/webarq/adminx.git"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"webarq/admin": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "auto"
	},
	"minimum-stability": "dev"
}
