{
	"name": "webarq/presence",
	"description": "A web application skeleton based on Laravel 4.2.11",
	"keywords": ["webarq", "presence"],
	"repositories": [
		{
			"type": "vcs",
			"url": "ssh://git@git.webarq.com:1903/webarq/site.git"
		},
		{
			"type": "vcs",
			"url": "ssh://git@git.webarq.com:1903/webarq/adminx.git"
		}
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"mews/captcha": "1.0.1",
		"webarq/admin": "1.2.x-dev"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "1.6.7"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"chmod -R 777 app/storage",
			"chmod 777 public/contents"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"chmod -R 777 app/storage",
			"chmod 777 public/contents"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "auto"
	},
	"minimum-stability": "dev"
}
