{
	"name": "webarq/presence",
	"description": "A web application boilerplate based on Laravel 4.0.9.",
	"keywords": ["webarq", "presence"],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@git2.webarq.com:webarq/site.git"
		},
		{
			"type": "vcs",
			"url": "git@git2.webarq.com:webarq/adminx.git"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"mews/captcha": "1.0.1",
		"webarq/admin": "1.0.x-dev"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "1.6.7"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "auto"
	},
	"minimum-stability": "dev"
}
